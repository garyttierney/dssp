	(macro set_use_removable_boolean ((type ARG1))
		(call sec.set_spec_boolean (ARG1 fs_use_removable.bool))
		(optional fs_set_use_removable_boolean_optional_seutil
			(call setsebool.exec (ARG1))))

	(macro unconfined_subj_type ((type ARG1))
		(typeattributeset unconfined_subj_type_attribute ARG1)))

