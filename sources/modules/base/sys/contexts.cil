(tunableif enable_mls
	(true
		(context sys (sys.id sys.role sys.subj (systemhigh systemhigh))))
	(false
		(context sys (sys.id sys.role sys.subj (systemlow systemlow)))))

(filecon "/var/log/boot\.log" file sys_var_log_file)
